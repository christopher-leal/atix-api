{
  "name": "atix-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf node_modules package-lock.json dist && npm i",
    "build": "npm run clean && npm run build-babel",
    "build-babel": "babel ./src -d ./dist",
    "dev": "nodemon -L --exec ./node_modules/.bin/babel-node -r dotenv/config  src/index.js",
    "start": "node -r dotenv/config  dist/index.js",
    "test": "jest --testTimeout=10000",
    "test:watch": "jest --watch --testTimeout=10000",
    "docs": "jsdoc -c jsdoc.json",
    "lint":"eslint ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/christopher-leal/atix-api.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/christopher-leal/atix-api/issues"
  },
  "homepage": "https://github.com/christopher-leal/atix-api#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "express-validator": "^6.14.0",
    "helmet": "^4.6.0",
    "morgan": "^1.10.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/node": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "jest": "^27.4.5",
    "jsdoc": "^3.6.7",
    "nodemon": "^2.0.15",
    "supertest": "^6.1.6"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFiles": [
      "dotenv/config"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/dist/"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ]
  },
  "engines": {
    "node": "16.13.1"
  }
}
